<p id="notice"><%= notice %></p>

<h1>File Listings</h1>

<table>
  <thead>
    <tr>
      <th>File name</th>
      <th>File path</th>
      <th>Last modified</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @file_listings.each do |file_listing| %>
      <tr>
        <td><%= file_listing.file_name %></td>
        <td><%= file_listing.file_path %></td>
        <td><%= file_listing.last_modified %></td>
        <td><%= link_to 'Show', file_listing %></td>
        <td><%= link_to 'Edit', edit_file_listing_path(file_listing) %></td>
        <td><%= link_to 'Destroy', file_listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New File Listing', new_file_listing_path %>
